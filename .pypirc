# open file %USERPROFILE%, touch .pypirc:
[distutils]
index-servers =
    nexus
    pypi    
    std.algorithm
    sympy.keras


[pypi]
username = __token__
password = pypi-******


[std.algorithm]
repository = https://upload.pypi.org/legacy/
username = __token__
password = pypi-******


[sympy.keras]
repository = https://upload.pypi.org/legacy/
username = __token__
password = pypi-******


[nexus]
repository = http://pypi.nexus.your-company.com/repository/pypi-internal/
username = your-username
password = your-password